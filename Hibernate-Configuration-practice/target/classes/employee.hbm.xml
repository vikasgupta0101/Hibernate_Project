<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="org.jsp.hibernateConfiguration.Employee" table="employee">

		<id name="id" column="id">
			<generator class="identity" />  <!-- identity -> Gives  the auto  Generated value-->
		</id>





		<property name="name" column="name" not-null="true"></property>
		<property name="aadharNo" column="aadharNo" unique="true"
			not-null="true"></property>
		<property name="phone" column="phone" unique="true"
			not-null="true"></property>
				<property name="salary" column="salary" unique="true"
			not-null="true"></property>
		

	</class>
</hibernate-mapping>